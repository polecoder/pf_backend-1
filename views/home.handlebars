<div>
  <h1>Productos</h1>
  <ul>
    {{#each products}}
      <li>
        <h2>{{this.title}}</h2>
        <span>UYU$ {{this.price}}</span>
        <p>{{this.description}}</p>
        <a href="#">Agregar al carrito</a>
      </li>
    {{/each}}
  </ul>
  {{#if hasPrevPage}}
    <a href="/products?page={{prevPage}}">Anterior</a>
  {{/if}}
  {{#if hasNextPage}}
    <a href="/products?page={{nextPage}}">Siguiente</a>
  {{/if}}
</div>